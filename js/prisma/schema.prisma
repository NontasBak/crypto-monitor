generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Measurement {
  id        Int    @id @default(autoincrement())
  symbol    Symbol
  price     Float
  size      Float
  timestamp BigInt
}

enum Symbol {
  BTC_USDT
  ADA_USDT
  ETH_USDT
  DOGE_USDT
  XRP_USDT
  SOL_USDT
  LTC_USDT
  BNB_USDT
}
