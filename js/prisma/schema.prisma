generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Measurement {
  id        Int    @id @default(autoincrement())
  symbol    String
  price     Float
  size      Float
  timestamp BigInt
}

model Average {
  id        Int    @id @default(autoincrement())
  symbol    String
  average   Float
  timestamp BigInt
}
